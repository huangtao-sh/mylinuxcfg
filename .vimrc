set guifont=YaHei\ Consolas\ Hybrid\ 12
set fenc=utf-8
set fencs=utf-8,usc-bom,euc-jp,gb18030,dbk,gb2312,cp936

set nocompatible
set history=100

set confirm

set clipboard+=unnamed

filetype on
filetype plugin on

filetype indent on

syntax on

set nobackup

set autoindent

set tabstop=4
set softtabstop=4
set shiftwidth=4

